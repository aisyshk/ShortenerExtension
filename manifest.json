{
    "manifest_version": 2,
    "name": "Aisys.dev Shortener",
    "version": "2.5",
    "permissions": [
        "contextMenus",
        "activeTab"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/img.png",
            "48": "images/img.png",
            "128": "images/img.png"
        }
    }
}